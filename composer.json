{
    "name": "yajra/laravel-acl",
    "description": "Laravel ACL is a simple role, permission ACL for Laravel Framework.",
    "keywords": [
        "yajra",
        "laravel",
        "acl",
        "user",
        "role",
        "permissions"
    ],
    "homepage": "https://github.com/yajra/laravel-acl",
    "license": "MIT",
    "authors": [
        {
            "name": "Arjay Angeles",
            "email": "aqangeles@gmail.com",
            "homepage": "http://yajrabox.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "^5.8|^6|^7",
        "illuminate/database": "^5.8|^6|^7"
    },
    "require-dev": {
        "phpunit/phpunit" : "^7.5|^8|^9",
        "orchestra/testbench": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "Yajra\\Acl\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yajra\\Acl\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        },
        "laravel": {
            "providers": [
                "Yajra\\Acl\\AclServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
